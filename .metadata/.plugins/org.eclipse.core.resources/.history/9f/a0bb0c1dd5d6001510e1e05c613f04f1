package boardCrawler;

import java.util.ArrayList;


public class BoardCrawler {

	public static void main(String[] args) {
		
		int i = 0;
		while(true) {
			if (i++ % 10 == 0) {
				Thread.sleep(1000);
				continue;
			}
			ArrayList<org.bson.Document> list = Crawler4Aggag.crwaling();
			dbInsert.insertDB(list);
		}
	}
}
